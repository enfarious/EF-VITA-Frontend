{
	"name": "ef-vita-frontend",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"description": "EF-VITA Tribe Management Frontend (API-contract-first)",
	"engines": {
		"node": ">=20.19.0 || >=22.12.0"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc -p tsconfig.json && vite build",
		"preview": "vite preview",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint": "eslint . --ext .ts,.tsx",
		"format": "prettier --write .",
		"test": "vitest run",
		"test:watch": "vitest",
		"openapi:types": "openapi-typescript contracts/openapi.yaml -o src/types/api.d.ts",
		"openapi:lint": "npx @redocly/cli lint contracts/openapi.yaml"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.59.0",
		"@tanstack/react-query-devtools": "^5.59.0",
		"@tanstack/react-router": "^1.87.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@eslint/js": "^9.15.0",
		"@tanstack/router-vite-plugin": "^1.145.2",
		"@types/node": "^25.0.3",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint": "^9.15.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.14",
		"jsdom": "^25.0.1",
		"openapi-typescript": "^6.7.3",
		"prettier": "^3.3.3",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.15.0",
		"vite": "^7.3.0",
		"vitest": "^4.0.16"
	},
	"volta": {
		"node": "20.19.4"
	}
}
